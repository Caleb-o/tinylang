# Namespacing
namespace Math {
	struct Point { x, y }
	struct PointF { x: float, y: float }
	struct PointI { x: int , y: int }
	
	function min(x, y) {}

	# Export is required as visibility isn't a thing
	export Point, PointF, PointI, min;
};

let smaller = Math.min(1, 2);


# Throwing and catching errors
function foo(flag: bool) {
	if flag {
		throw "Oh no!";
	}
};

catch foo(false) as err {
	print(err);
};