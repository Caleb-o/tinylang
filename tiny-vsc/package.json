{
    "name": "tinylang",
    "displayName": "tinylang",
    "description": "A scripting language",
    "author": {
        "name": "Caleb",
        "url": "https://github.com/Caleb-o/tinylang"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {},
    "contributes": {
        "languages": [
            {
                "id": "tinylang",
                "aliases": [
                    "Tiny",
                    "tiny"
                ],
                "extensions": [
                    ".tiny"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tinylang",
                "scopeName": "source.tinylang",
                "path": "./syntaxes/tiny.tmLanguage.json"
            }
        ]
    }
}